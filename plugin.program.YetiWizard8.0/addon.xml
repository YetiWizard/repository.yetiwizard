<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.program.YetiWizard8.0" name="Yeti Wizard" version="8.0" provider-name="YetiWizard">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.addon.repository" name="yetiwizardrepo">
        <info>https://github.com/YetiWizard/repository.yetiwizard/blob/main/repository.yetiwizard/addons.xml</info>
        <checksum>https://github.com/YetiWizard/repository.yetiwizard/blob/main/repository.yetiwizard/addons.xml.md5</checksum>
        <datadir zip="true">https://raw.githubusercontent.com/yetiwizard/yetiwizardrepo/master/</datadir>
    </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Wizard</summary>
    <description lang="en">Yeti Wizard allows users to download and install Yeti Build</description>
    <platform>all</platform>
  </extension>
</addon>
