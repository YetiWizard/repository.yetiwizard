<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.program.YetiWizard8.0" name="yeti wizard" version="8.0" provider-name="yetiwizard">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.addon.repository" name="yetiwizardrepo">
        <info>https://github.com/YetiWizard/repository.yetiwizard/blob/main/repository.yetiwizard/addons.xml</info>
        <checksum>https://github.com/YetiWizard/repository.yetiwizard/blob/main/repository.yetiwizard/addons.xml.md5</checksum>
        <datadir zip="true">https://github.com/YetiWizard/repository.yetiwizard/tree/main/repository.yetiwizard/</datadir>
    </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Repository for Yeti Wizard Kodi 19</summary>
    <description lang="en">Yeti Wizard is used to install Yeti Build version 8 for Kodi 19</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.yetiwizardrepo" name="[COLOR white]yetiwizardrepo[/COLOR]" version="1.0" provider-name="yetiwizard">
    <extension point="xbmc.addon.repository" name="yetiwizardrepo">
        <info>https://github.com/YetiWizard/repository.yetiwizard/blob/main/repository.yetiwizard/addons.xml</info>
        <checksum>https://github.com/YetiWizard/repository.yetiwizard/blob/main/repository.yetiwizard/addons.xml.md5</checksum>
        <datadir zip="true">https://github.com/YetiWizard/repository.yetiwizard/tree/main/repository.yetiwizard/</datadir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Repository for Yeti Wizard Kodi 19</summary>
        <description></description>
        <disclaimer></disclaimer>
        <platform>all</platform>
    </extension>
</addon>

</addons>
